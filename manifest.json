{
    "name": "Bookmark",
    "version": "1.0",
    "description": "Save your bookmarks with sync options",
    "manifest_version": 3,
    "icons": {
      "16": "./favicon/favicon-16x16.png",
      "48": "./favicon/favicon.ico",
      "128":"./favicon/android-chrome-512x512.png"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    }
    
    
  }